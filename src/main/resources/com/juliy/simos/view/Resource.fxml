<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXListView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<HBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="640.0"
      spacing="10.0" xmlns="http://javafx.com/javafx/18"
      fx:controller="com.juliy.simos.controller.ResourceController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <VBox prefHeight="200.0" prefWidth="200.0" spacing="10.0">
        <HBox alignment="CENTER" prefHeight="40.0" prefWidth="200.0">
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="资源A：">
                <font>
                    <Font size="16.0"/>
                </font>
            </Text>
            <Text fx:id="txtFreeA" fill="GREEN" strokeType="OUTSIDE"
                  strokeWidth="0.0" text="10">
                <font>
                    <Font size="16.0"/>
                </font>
            </Text>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="/">
                <font>
                    <Font size="16.0"/>
                </font>
            </Text>
            <Text fx:id="txtTotalA" strokeType="OUTSIDE" strokeWidth="0.0"
                  text="10">
                <font>
                    <Font size="16.0"/>
                </font>
            </Text>
        </HBox>
        <JFXListView fx:id="listA"/>
    </VBox>
    <VBox layoutX="20.0" layoutY="20.0" prefHeight="200.0" prefWidth="200.0"
          spacing="10.0">
        <HBox alignment="CENTER" prefHeight="40.0" prefWidth="200.0">
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="资源B：">
                <font>
                    <Font size="16.0"/>
                </font>
            </Text>
            <Text fx:id="txtFreeB" fill="GREEN" strokeType="OUTSIDE"
                  strokeWidth="0.0" text="10">
                <font>
                    <Font size="16.0"/>
                </font>
            </Text>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="/">
                <font>
                    <Font size="16.0"/>
                </font>
            </Text>
            <Text fx:id="txtTotalB" strokeType="OUTSIDE" strokeWidth="0.0"
                  text="10">
                <font>
                    <Font size="16.0"/>
                </font>
            </Text>
        </HBox>
        <JFXListView fx:id="listB"/>
    </VBox>
    <VBox layoutX="20.0" layoutY="20.0" prefHeight="200.0" prefWidth="200.0"
          spacing="10.0">
        <HBox alignment="CENTER" prefHeight="40.0" prefWidth="200.0">
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="资源C：">
                <font>
                    <Font size="16.0"/>
                </font>
            </Text>
            <Text fx:id="txtFreeC" fill="GREEN" strokeType="OUTSIDE"
                  strokeWidth="0.0" text="10">
                <font>
                    <Font size="16.0"/>
                </font>
            </Text>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="/">
                <font>
                    <Font size="16.0"/>
                </font>
            </Text>
            <Text fx:id="txtTotalC" strokeType="OUTSIDE" strokeWidth="0.0"
                  text="10">
                <font>
                    <Font size="16.0"/>
                </font>
            </Text>
        </HBox>
        <JFXListView fx:id="listC"/>
    </VBox>
</HBox>
